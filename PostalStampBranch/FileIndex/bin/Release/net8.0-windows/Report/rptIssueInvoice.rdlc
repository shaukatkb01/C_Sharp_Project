  <Code>Public Function IntegerToWords(ByVal amount As Decimal) As String

    Dim intPart As Integer = Math.Floor(amount)
    Dim decPart As Integer = CInt((amount - intPart) * 100)

    Dim words As String = NumberToWords(intPart)

    If decPart &gt; 0 Then
        words &amp;= " and " &amp; NumberToWords(decPart) &amp; " Paisa"
    End If

    Return words &amp; " Only"
End Function


Private Function NumberToWords(ByVal num As Integer) As String

    Dim units() As String = {"Zero", "One", "Two", "Three", "Four", "Five", "Six", "Seven", "Eight", "Nine",
                             "Ten", "Eleven", "Twelve", "Thirteen", "Fourteen", "Fifteen",
                             "Sixteen", "Seventeen", "Eighteen", "Nineteen"}

    Dim tens() As String = {"Zero", "Ten", "Twenty", "Thirty", "Forty",
                            "Fifty", "Sixty", "Seventy", "Eighty", "Ninety"}

    If num &lt; 20 Then
        Return units(num)

    ElseIf num &lt; 100 Then
        Return tens(num \ 10) &amp; If(num Mod 10 &gt; 0, "-" &amp; units(num Mod 10), "")

    ElseIf num &lt; 1000 Then
        Return units(num \ 100) &amp; " Hundred" &amp;
               If(num Mod 100 &gt; 0, " " &amp; NumberToWords(num Mod 100), "")

    ElseIf num &lt; 1000000 Then
        Return NumberToWords(num \ 1000) &amp; " Thousand" &amp;
               If(num Mod 1000 &gt; 0, " " &amp; NumberToWords(num Mod 1000), "")

    Else
        Return num.ToString()
    End If

End Function</Code>
